@page
@model LaborVolumeCalculator.Pages.OkrInnovationRates.DeleteModel

@{
    ViewData["Title"] = "Удалить";
}

<h1>Удалить</h1>

<h3>Вы действительто хотите удалить этот объект?</h3>
<div>
    <h4>@Html.DisplayNameFor(model => model.OkrInnovationRate)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OkrInnovationRate.OkrInnovationProperty)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OkrInnovationRate.OkrInnovationProperty.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OkrInnovationRate.DeviceComposition)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OkrInnovationRate.DeviceComposition.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OkrInnovationRate.Value)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OkrInnovationRate.Value)
        </dd>
    </dl>

    <form method="post">
        <input type="hidden" asp-for="OkrInnovationRate.OkrInnovationPropertyID" />
        <input type="hidden" asp-for="OkrInnovationRate.DeviceCompositionID" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-page="./Index">Вернуться к списку</a>
    </form>
</div>
