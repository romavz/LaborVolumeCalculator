@page
@model LaborVolumeCalculator.Pages.DeviceComplexityRates.DeleteModel

@{
    ViewData["Title"] = "Удалить";
}

<h1>Удалить</h1>

<h3>Вы действительто хотите удалить этот объект?</h3>
<div>
    <h4>@Html.DisplayNameFor(model => model.DeviceComplexityRate)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DeviceComplexityRate.DeviceComposition)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DeviceComplexityRate.DeviceComposition.ID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DeviceComplexityRate.DeviceCountRange)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DeviceComplexityRate.DeviceCountRange.ID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DeviceComplexityRate.Value)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DeviceComplexityRate.Value)
        </dd>
    </dl>

    <form method="post">
        <input type="hidden" asp-for="DeviceComplexityRate.ID" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-page="./Index">Вернуться к списку</a>
    </form>
</div>
